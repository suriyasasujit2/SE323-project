@model WatDuangDee.Domain.Entities.History
@{
    ViewBag.Title = "History";
}
 <div class="panel panel-primary">
    <div class="panel-heading">
        <h3>History (ประวัติ ความเป็นมา)</h3>
    </div>
    <div class="panel-body">
        <div class="col-md-6">
            <center>
                @if (Model.ImageData != null)
                {

                    <img width="30%" height="30% " class="img-thumbnail" src="@Url.Action("GetImage", "Menu", new { id = Model.HistoryId,Type=1 })" />


                }
                else
                {
                    @Html.Raw("No")
                }
            </center>
        </div>
        <div class="col-md-6">
            <h4>
            @Model.HistoryDetail</h4>
        </div>
    </div>
</div>
